{
    "name": "Multiple Cards System Compiler", 
    "description": [
        "This is simple example showcasing the P2P and H2C mode of application code controlling a multi-card accelerator design in system compiler environment."
    ],
    "flow": "vitis",
    "keywords": [
        "vpp::p2p",
        "vpp::h2c",
        "vpp::squeue",
        "add_card"
    ], 
    "key_concepts": [
        "Multiple Cards",
        "P2P",
        "H2C",
        "Syncronization" 
    ],
    "platform_blocklist": [
        "nodma",
        "zc",
        "vck",
        "u2_",
        "u25_",
        "aws",
        "u280"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ],
    "match_makefile" : "false",
    "match_ini" : "false", 
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "./src/main.cpp"
            ] 
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "hls_top", 
                    "location": "src/xacc.cpp"
                }
            ], 
            "name": "hls_top"
        }
    ],
    "launch": [
        {
            "cmd_args": "0 Makefile ./src/xacc.hpp ./src/xacc.cpp ./src/main.cpp",
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "features": [
            "p2p"
        ],
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
