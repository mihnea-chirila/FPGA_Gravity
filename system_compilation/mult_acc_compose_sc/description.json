{
    "name": "Multiple Accelerators System Compiler", 
    "description": [
        "In this example we have multiple accelerators in one xclbin, and compose them into a pipeline, with or without CPU processing in-between the PEs."
    ],
    "flow": "vitis",
    "keywords": [
        "sync_outputs",
        "custom_sync_outputs"
    ], 
    "key_concepts": [
        "Multiple Accelerators"
    ],
    "platform_blocklist": [
        "zc",
        "vck",
        "aws",
        "nodma"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ],
    "match_makefile" : "false", 
    "match_ini" : "false", 
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "./src/main.cpp"
            ] 
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "hls_top", 
                    "location": "src/filter.cpp"
                }
            ], 
            "name": "filter"
        }
    ],
    "launch": [
        {
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
